<Registration>:
    kembali_kerumah:kembali_kerumah
    # lp_properties:[username,password,email]
    canvas.before:
        Rectangle:
            pos: self.pos
            size: self.size
            source:'py/logo/bgregist.jpg'
    MDIconButton:
        id:kembali_kerumah

        pos_hint: {'left': 1, 'top': 1}
        icon:"arrow-left-bold-outline"
        on_release : root.back("berhasil balik")

 
    MDCard:
        orientation: "vertical"
        size_hint: None, None
        size: root.width-root.width/5,root.height-root.height/5
        pos_hint: {"center_x": .5, "center_y": .5}
        # pos:self.pos
        radius: dp(15),dp(15),dp(15),dp(15)

    MDBoxLayout:
        orientation: "horizontal"      
        spacing : 50
        size_hint:None,None
        pos_hint: { 'top': .95,'center_x':.45}
        
        MDIconButton:
            icon: "google-plus.png"
            user_font_size :"24sp"
            on_release : print("testing")
        MDIconButton:
            icon: "facebook.png"
            user_font_size :"24sp"
            on_release : print("testing")
    MDLabel :
        text:"atau"
        pos_hint: {"top": .85,'center_x':.5}
        halign: 'center'
        size_hint:None,None
        spacing:10
    # MDBoxLayout:
    #     orientation: 'vertical'
    #     size_hint_x: None
    #     size_hint_y: None
    #     pos_hint: {'center_x': .5, 'top': .8}

        # pos_hint: {'center_x': 0.5, 'center_y': 0.3}
        # width: dp(200)

    #  Dibawah adalah form registrasi
    #  Dibawah adalah form registrasi
    #  Dibawah adalah form registrasi
    
    MDTextField:
        id: first_name
        color_mode: 'custom'
        line_color_focus: .4, .4, .3, 1
        hint_text: "Username"
        helper_text: "Wajib diisi"
        margin_bottom: dp(20)
        on_focus:root.text_required_on_focus()
        required: True
        pos_hint: {'center_x': .5, 'top': .75}
        size_hint:.5,None
        helper_text_mode: "on_error"
        padding:20,20,20,20

    MDTextField:
        id: passwordmy
        hint_text: "Password"
        margin_bottom: dp(20)
        password: True
        # required: True
        pos_hint: {'center_x': .5, 'top': .68}
        size_hint:.5,None
        helper_text_mode: "on_error"
 
    MDIconButton:
        icon: "eye-off"
        ripple_scale: .5
        # pos: text_field.width - self.width + dp(8), 0
        on_release:
            self.icon = "eye" if self.icon == "eye-off" else "eye-off"
            passwordmy.password = False if passwordmy.password is True else True
        pos_hint: {'center_x': .8, 'top': .67}
        size_hint:.5,None

    MDTextField:
        id: email_signup
        hint_text: "Email"
        margin_bottom: dp(20)
        # required: True
        helper_text_mode: "on_error"
        pos_hint: {'center_x': .5, 'top': .61}
        size_hint:.5,None
    MDTextField:
        id: password_signup
        hint_text: "Password"
        margin_bottom: dp(20)
        # required: True
        helper_text_mode: "on_error"
        password: True
        pos_hint: {'center_x': .5, 'top': .54}
        size_hint:.5,None
